<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitdontquit">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pingwing"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pingwing"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitDontQuit">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeScreen" />
        <activity android:name=".MainActivity" />
        <activity android:name=".WorkoutsActivity" />
        <activity android:name=".StartMountainClimbing" />
    </application>

</manifest>